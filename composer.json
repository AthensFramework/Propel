{
  "name": "athens/propel",
  "description": "Modern web framework in PHP for academic environments",
  "license": "MIT",
  "autoload": {
    "classmap": [
      "src/"
    ]
  },
  "autoload-dev": {
    "classmap": [
      "test/"
    ]
  },
  "require": {
    "propel/propel": "~2.0@dev",
    "athens/core": "0.*"
  },
  "require-dev": {
    "phpunit/phpunit": "5.*",
    "phpdocumentor/phpdocumentor": "2.*",
    "evert/phpdoc-md" : "~0.2.0",
    "codeclimate/php-test-reporter": "dev-master",
    "squizlabs/php_codesniffer": "2.*",
    "athens/standard": "*"
  },
  "type" : "propel-behavior",
  "extra": {
    "name": "html-field",
    "class": "\\Athens\\Core\\Behavior\\HTMLField"
  }
}
